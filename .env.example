# Supabase (필수)
NEXT_PUBLIC_SUPABASE_URL=https://<project-ref>.supabase.co
NEXT_PUBLIC_SUPABASE_PUBLISHABLE_KEY=<supabase-publishable-key>

# Supabase Service Role (서버 전용 - 절대 클라이언트에 노출 금지)
SUPABASE_SERVICE_ROLE_KEY=<supabase-service-role-key>

# Cron 인증 (API Route 보호용)
CRON_SECRET=<랜덤-시크릿-생성>

# Slack 알림 (선택)
SLACK_WEBHOOK_URL=<slack-webhook-url>

# Playwright 테스트 계정 (선택 - E2E 테스트용)
# 소셜 로그인 테스트를 위해 Supabase admin API로 자동 생성됨
TEST_SOCIAL_USER_EMAIL=test-social@lifeboard.dev
